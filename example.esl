type Tuple = (Expr, Expr)
symbol foo: Expr -> Expr
symbol foo: Expr, Expr -> Expr
symbol bar: Tup -> Expr

symbol true: -> Value
symbol false: -> Value
symbol null: -> Value

type Member = (String, Value)
symbol object: Member* -> Value
symbol array: Value* -> Value

type Signed = (Sign, Digit*)
type Fraction = Digit*
type Exponent = Signed
symbol number: Signed, Fraction?, Exponent? -> Value

symbol Unit: -> Unit
